class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:

        dic = {}
        l, r = 0, 0
        res = 0
        while r < len(s):
            if s[r] in dic:
                res = max(res, r-l)
                l = max(dic[s[r]]+1,l)
            dic[s[r]] = r
            r += 1
        res = max(res, r-l)
        return res
