class Solution:
    def isPalindrome(self, x: int) -> bool:
        if x < 0:
            return False
        else:
            x = str(x)
            i = 0
            while i < len(x)//2:
                if x[i] != x[len(x)-1-i]:
                    return False
                i += 1
            return True
            
        
